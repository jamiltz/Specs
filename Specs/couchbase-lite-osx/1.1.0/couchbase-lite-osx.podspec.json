{
  "name": "couchbase-lite-ios",
  "version": "1.1.0",
  "license": {
    "type": "Apache License, Version 2.0",
    "file": "LICENSE.txt"
  },
  "homepage": "http://mobile.couchbase.com",
  "summary": "An embedded syncable NoSQL database for osx apps.",
  "authors": {
    "Jens Alfke": "jens@couchbase.com"
  },
  "source": {
    "http": "http://packages.couchbase.com/builds/mobile/ios/1.1.0/1.1.0-18/couchbase-lite-macosx-community_1.1.0-18.zip"
  },
  "preserve_paths": [
    "CouchbaseLite.framework",
    "CouchbaseLiteListener.framework",
    "LICENSE.txt"
  ],
  "osx": {
    "vendored_frameworks": [
      "CouchbaseLite.framework",
      "CouchbaseLiteListener.framework"
    ]
  },
  "platforms": {
    "osx": "10.8"
  },
  "frameworks": [
    "CFNetwork",
    "Security",
    "SystemConfiguration"
  ],
  "libraries": [
    "sqlite3",
    "z"
  ],
  "xcconfig": {
    "OTHER_LDFLAGS": "-ObjC",
    "LD_RUNPATH_SEARCH_PATHS": "@loader_path/../Frameworks"
  },
  "requires_arc": true
}
